{
  "name": "symfony/shop",
  "type": "project",
  "license": "MIT",
  "description": "Garden shop application",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.16",
    "fakerphp/faker": "^1.23",
    "gesdinet/jwt-refresh-token-bundle": "^1.2",
    "lexik/jwt-authentication-bundle": "^2.20",
    "nelmio/api-doc-bundle": "^4.12",
    "ramsey/uuid": "*",
    "scienta/doctrine-json-functions": "~4.3",
    "symfony/asset": "6.3.*",
    "symfony/console": "6.3.*",
    "symfony/dotenv": "6.3.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.3.*",
    "symfony/mailer": "6.3.*",
    "symfony/messenger": "6.3.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/property-access": "6.3.*",
    "symfony/runtime": "6.3.*",
    "symfony/security-bundle": "6.3.*",
    "symfony/serializer": "6.3.*",
    "symfony/twig-bundle": "6.3.*",
    "symfony/validator": "6.3.*",
    "symfony/yaml": "6.3.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UI\\": "src/UI",
      "Auth\\": "src/Auth",
      "Common\\": "src/Common"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Common\\": "tests/Common",
      "Test\\Functional\\Auth\\": "tests/Functional/Auth"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.3.*"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^7.2",
    "doctrine/doctrine-fixtures-bundle": "^3.5",
    "liip/test-fixtures-bundle": "^2.0.0",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "6.3.*",
    "symfony/css-selector": "6.3.*",
    "symfony/maker-bundle": "^1.51",
    "symfony/phpunit-bridge": "^6.3"
  }
}
